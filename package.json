{
  "name": "devops-test",
  "version": "0.0.0",
  "description": "Basic Hello World app for testing",
  "private": true,
  "main": "src/server.ts",
  "workspaces": [
    "app"
  ],
  "type": "module",
  "scripts": {
    "build": "node app/src/util/build.ts",
    "commitlint": "commitlint --from=$(git rev-parse $(git cherry origin/main | head -n 1 | cut -c 3-)^1)",
    "dev": "nodemon --watch app/src --ext js,ts,scss,html,njk --exec \"npm run start\"",
    "start": "npm run build && node app/src/server.ts",
    "test": "node --test"
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "^2.2.2",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-validator": "^7.3.1",
    "govuk-frontend": "^5.14.0",
    "nunjucks": "^3.2.4",
    "pino": "^10.3.0",
    "pino-pretty": "^13.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.0",
    "@types/nunjucks": "^3.2.6",
    "nodemon": "^3.1.11",
    "sass": "^1.97.3",
    "typescript": "^5.9.3"
  }
}